{
    "version": "0.2.0",
    "command": "node",
    "isShellCommand": true,
    "args": [
        "--harmony"
    ],
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch via NPM",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "debug"
            ],
            "port": 4000,
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}